{
  "name": "next-mui-archetype",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev-p-3001": "next dev -p 3001",
    "ssl-proxy": "npx local-ssl-proxy --source 3000 --target 3001",
    "build": "next build",
    "start": "next start",
    "start-p-3001": "next start -p 3001",
    "lint": "next lint",
    "dev-https": "npm-run-all --parallel ssl-proxy dev-p-3001",
    "start-https": "npm-run-all --parallel ssl-proxy start-p-3001",
    "recreate-database": "npx sequelize-cli db:drop --url '%npm_config_url%' && npx sequelize-cli db:create --url '%npm_config_url%' && npx sequelize-cli db:migrate --url '%npm_config_url%' && npx sequelize-cli db:seed:all --url '%npm_config_url%'",
    "docker-db-container-create-production": "docker compose -f docker/db-engine-production/docker-compose.yml --env-file .env.production build",
    "docker-db-container-start-production": "docker compose -f docker/db-engine-production/docker-compose.yml --env-file .env.production up -d",
    "docker-db-container-stop-production": "docker compose -f docker/db-engine-production/docker-compose.yml --env-file .env.production down",
    "docker-db-container-create-development": "docker compose -f docker/db-engine-development/docker-compose.yml --env-file .env.development build",
    "docker-db-container-start-development": "docker compose -f docker/db-engine-development/docker-compose.yml --env-file .env.development up -d",
    "docker-db-container-stop-development": "docker compose -f docker/db-engine-development/docker-compose.yml --env-file .env.development down"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.383.0",
    "@aws-sdk/client-ses": "^3.382.0",
    "@aws-sdk/s3-request-presigner": "^3.383.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.3",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@hookform/resolvers": "^3.1.1",
    "@mui/lab": "^5.0.0-alpha.136",
    "@mui/material": "^5.14.3",
    "@mui/x-data-grid": "^6.9.0",
    "@next-auth/sequelize-adapter": "^1.0.8",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "20.3.2",
    "@types/react": "18.2.14",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "18.2.6",
    "@types/react-email-editor": "^1.5.1",
    "@uidotdev/usehooks": "^2.1.0",
    "autoprefixer": "10.4.14",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "image-conversion": "^2.1.1",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.4.3",
    "next": "^13.4.19",
    "next-auth": "^4.22.4",
    "next-themes": "^0.2.1",
    "notistack": "^3.0.1",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-color": "^2.19.3",
    "react-currency-input-field": "^3.6.11",
    "react-dom": "18.2.0",
    "react-email-editor": "^1.7.8",
    "react-hook-form": "^7.45.1",
    "react-image-crop": "^10.1.5",
    "react-material-ui-carousel": "^3.4.2",
    "react-number-format": "^5.2.2",
    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.5",
    "tailwindcss": "^3.3.3",
    "typescript": "5.1.6",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "eslint": "^8.46.0",
    "eslint-config-next": "13.4.13",
    "npm-run-all": "^4.1.5"
  }
}
